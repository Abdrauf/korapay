{
  "name": "korapay",
  "version": "1.0.0",
  "description": "KORAPAY TECHNICAL TEST",
  "main": "index.js",
  "scripts": {
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "chrome": "cypress run  --headless --browser chrome",
    "firefox": "cypress run  --headless --browser firefox",
    "edge": "cypress run  --headless --browser edge",
    "report:merge": "mochawesome-merge --reportDir cypress/results/json > cypress/results/mochawesome-bundle.json",
    "report:generate": "marge cypress/results/mochawesome-bundle.json -o cypress/results/html",
    "report:copyScreenshots": "cp -r cypress/screenshots cypress/results/html/screenshots"
  },
  "keywords": [
    "cypress",
    "korapay",
    "automation"
  ],
  "author": "ABDRAUF SEGUN",
  "license": "ISC",
  "dependencies": {
    "cypress": "^7.1.0",
    "cypress-cucumber-preprocessor": "^4.0.3"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "devDependencies": {
    "cypress-dark": "^1.7.14",
    "cypress-multi-reporters": "^1.5.0",
    "mocha": "^8.3.2",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0"
  }
}
